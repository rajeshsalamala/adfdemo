{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-psx"
		},
		"lk_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lk_blob'"
		},
		"lk_sink_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lk_sink_adls'"
		},
		"lk_src_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lk_src_sql'",
			"defaultValue": "Integrated Security=False;Data Source=localhost,49170;Initial Catalog=rajeshDb;User ID=rajesh"
		},
		"lk_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-psx-rajesh.vault.azure.net/"
		},
		"lk_sink_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspsxrajesh.dfs.core.windows.net/"
		},
		"triggerEvent_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5b20eebf-fc79-4039-add2-9e212995b467/resourceGroups/rg-psx/providers/Microsoft.Storage/storageAccounts/adlspsxrajesh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-psx_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lk_blob_connectionString": {
						"value": "[parameters('lk_blob_connectionString')]"
					},
					"lk_sink_adls_accountKey": {
						"value": "[parameters('lk_sink_adls_accountKey')]"
					},
					"lk_src_sql_connectionString": {
						"value": "[parameters('lk_src_sql_connectionString')]"
					},
					"lk_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('lk_kv_properties_typeProperties_baseUrl')]"
					},
					"lk_sink_adls_properties_typeProperties_url": {
						"value": "[parameters('lk_sink_adls_properties_typeProperties_url')]"
					},
					"triggerEvent_properties_typeProperties_scope": {
						"value": "[parameters('triggerEvent_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}